         }).then((result) => {\n              if (result.isConfirmed) {\n                  axios.delete(`/recipes/${id}`)\n                  .then( response => {\n                      Swal.fire({\n                          icon: 'success',\n                          title: 'Recipe deleted successfully!',\n                          showConfirmButton: false,\n                          timer: 1500\n                      })\n                      this.fetchRecipesList();\n                      return response\n                  })\n                  .catch(error => {\n                      Swal.fire({\n                           icon: 'error',\n                          title: 'An Error Occured!',\n                          showConfirmButton: false,\n                          timer: 1500\n                      })\n                      return error\n                  });\n              }\n            })\n      }\n    },\n  };\n  </script>"],"mappings":";AAuDE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,IAAG,MAAO,aAAY;AAE7B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACPC,OAAO,EAAE,EAAE;MACXC,IAAI,EAAC,CAAC;QAAE,IAAI,EAAE,CAAC;QAAE,MAAM,EAAE,QAAQ;QAAE,aAAa,EAAE,IAAI;QAAE,YAAY,EAAE,IAAI;QAAE,aAAa,EAAE,IAAI;QAAE,UAAU,EAAE;MAAS,CAAC,EAAE;QAAE,IAAI,EAAE,CAAC;QAAE,MAAM,EAAE,QAAQ;QAAE,aAAa,EAAE,IAAI;QAAE,YAAY,EAAE,IAAI;QAAE,aAAa,EAAE,IAAI;QAAE,UAAU,EAAE;MAAS,CAAC,CAAC;MAC3OC,KAAK,EAAC;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,KAAI,GAAIC,IAAI,CAACC,KAAK,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,KAAK,CAAC;IAClD;IACA;;IAEAL,KAAK,CAACM,OAAO,CAACC,UAAS,IAAK;MAC1BC,OAAO,CAACC,GAAG,CAACF,UAAU,CAAC;MACvB,IAAI,CAACT,KAAK,CAACY,IAAI,CAACH,UAAU,CAAC;IAC7B,CAAC,CAAC;IACAlB,KAAK,CAACsB,IAAI,CAAC,4BAA4B,EAAE,IAAI,CAACb,KAAM,EACjDc,IAAI,CAACC,QAAO,IAAK;MAChB,IAAIC,WAAU,GAAID,QAAQ,CAACnB,IAAG;MAC9BoB,WAAW,CAACR,OAAO,CAACS,OAAM,IAAK;QAC7B,IAAI,CAACnB,OAAO,CAACc,IAAI,CAACK,OAAO,CAAC;MAC5B,CAAC,CAAC;MA