 <layout-div>\n      <h2 class=\"text-center mt-5 mb-3\">Create New Recipe</h2>\n      <div class=\"card\">\n          <div class=\"card-header\">\n              <router-link \n                  class=\"btn btn-outline-info float-right\"\n                  to=\"/\">View All Recipes\n              </router-link>\n          </div>\n          <div class=\"card-body\">\n              <form>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"name\">Name</label>\n                      <input \n                          v-model=\"recipe.name\"\n                          type=\"text\"\n                          class=\"form-control\"\n                          id=\"name\"\n                          name=\"name\"/>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"preparation\">Preparation</label>\n                      <textarea \n                          v-model=\"recipe.preparation\"\n                          class=\"form-control\"\n                          id=\"preparation\"\n                          rows=\"6\"\n                          name=\"preparation\"></textarea>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"time\">Prep Time</label>\n                      <input \n                          v-model=\"recipe.time\"\n                          type=\"number\"\n                          class=\"form-control\"\n                          id=\"time\"\n                          name=\"time\"/>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"serve\">Serves</label>\n                      <input \n                          v-model=\"recipe.serve\"\n                          type=\"number\"\n                          class=\"form-control\"\n                          id=\"serve\"\n                          name=\"serve\"/>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"difficulty\">Difficulty</label>\n                      <input \n                          v-model=\"recipe.difficulty\"\n                          type=\"number\"\n                          class=\"form-control\"\n                          id=\"difficulty\"\n                          name=\"difficulty\"/>\n                  </div>\n\n                  <thead>\n                      <tr>\n                        <th>Ingredient</th>\n                        <th>Quantity</th>\n                      </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"row in rows\" :key=\"row.index\">\n                      <td>\n                        <InputText />\n                      </td>\n                      <td>\n                        <InputText />\n                      </td>\n                    </tr>\n                  </tbody>\n\n                  <button \n                      @click=\"handleSave()\"\n                      :disabled=\"isSaving\"\n                      type=\"button\"\n                      class=\"btn btn-outline-primary mt-3\">\n                      Save Recipe\n                  </button>\n              </form>\n          </div>\n      </div>\n    </layout-div>\n  </template>\n   \n  <script>\n  import axios from 'axios';\n  import LayoutDiv from '../components/LayoutDiv.vue';\n  import Swal from 'sweetalert2'\n\n  export default {\n    name: 'RecipeCreate',\n    components: {\n      LayoutDiv\n    },\n    setup() {\n        const rows = ref([])\n        const addRow = () => {\n          rows.value.push([{\n              \"ingredient\": \"\",\n              \"quantity\": \"\",\n          },\n          ]);\n          console.log(rows.value);\n      } \n      return {\n        rows,\n        addRows\n      }  \n    },\n    data() {\n      return {\n        recipe: {\n          name: '',\n          preparation: '',\n          time: '',\n          serve: '',\n          difficulty: ''\n        },\n        isSaving:false,\n      };\n    },\n    methods: {\n      handleSave() {\n          this.isSaving = true\n          axios.post('/recipes', this.recipe)\n            .then(response => {\n              Swal.fire({\n                  icon: 'success',\n                  title: 'Recipe saved successfully!',\n                  showConfirmButton: false,\n                  timer: 1500\n              })\n              this.isSaving = false\n              this.recipe.name = \"\"\n              this.recipe.preparation = \"\"\n              this.recipe.time = \"\"\n              this.recipe.serve = \"\"\n              this.recipe.difficulty = \"\"\n              return response\n            })\n            .catch(error => {\n              this.isSaving = false\n              Swal.fire({\n                  icon: 'error',\n                  title: 'An Error Occured!',\n                  showConfirmButton: false,\n                  timer: 1500\n              })\n              return error\n            });\n      },\n    },\n  };\n  </script>"],"mappings":";AAyFE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,IAAG,MAAO,aAAY;AAE7B,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,KAAKA,CAAA,EAAG;IACJ,MAAMC,IAAG,GAAIC,GAAG,CAAC,EAAE;IACnB,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBF,IAAI,CAACG,KAAK,CAACC,IAAI,CAAC,CAAC;QACb,YAAY,EAAE,EAAE;QAChB,UAAU,EAAE;MAChB,CAAC,CACA,CAAC;MACFC,OAAO,CAACC,GAAG,CAACN,IAAI,CAACG,KAAK,CAAC;IAC3B;IACA,OAAO;MACLH,IAAI;MACJO;IACF;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNZ,IAAI,EAAE,EAAE;QACRa,WAAW,EAAE,EAAE;QACfC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,UAAU,EAAE;MACd,CAAC;MACDC,QAAQ,EAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACT,IAAI,CAACF,QAAO,GAAI,IAAG;MACnBpB,KAAK,CAACuB,IAAI,CAAC,UAAU,EAAE,IAAI,CAACR,MAAM,EAC/BS,IAAI,CAACC,QAAO,IAAK;QAChBvB,IAAI,CAACwB,IAAI,CAAC;UACNC,IAAI,EAAE,SAAS;UACfC,KAAK,EAAE,4BAA4B;UACnCC,