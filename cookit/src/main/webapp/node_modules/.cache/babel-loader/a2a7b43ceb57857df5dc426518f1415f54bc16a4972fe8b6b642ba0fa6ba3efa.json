{"ast":null,"code":"import axios from 'axios';\nimport LayoutDiv from '../LayoutDiv.vue';\nimport Swal from 'sweetalert2';\nexport default {\n  name: 'ProjectList',\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      posts: [],\n      projects: []\n    };\n  },\n  created() {\n    this.fetchProjectList();\n  },\n  methods: {\n    fetchProjectList() {\n      axios.get('/api/projects').then(response => {\n        this.projects = response.data;\n        return response;\n      }).catch(error => {\n        return error;\n      });\n    },\n    handleDelete(id) {\n      Swal.fire({\n        title: 'Are you sure?',\n        text: \"You won't be able to revert this!\",\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#3085d6',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Yes, delete it!'\n      }).then(result => {\n        if (result.isConfirmed) {\n          axios.delete(`/api/projects/${id}`).then(response => {\n            Swal.fire({\n              icon: 'success',\n              title: 'Project deleted successfully!',\n              showConfirmButton: false,\n              timer: 1500\n            });\n            this.fetchProjectList();\n            return response;\n          }).catch(error => {\n            Swal.fire({\n              icon: 'error',\n              title: 'An Error Occured!',\n              showConfirmButton: false,\n              timer: 1500\n            });\n            return error;\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","LayoutDiv","Swal","name","components","data","posts","projects","created","fetchProjectList","methods","get","then","response","catch","error","handleDelete","id","fire","title","text","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","isConfirmed","delete","showConfirmButton","timer"],"sources":["/Volumes/SDDisk2/workspace/IFRS/ES3/SSI-ESIII/cookit/src/main/webapp/src/views/ProjectList.vue"],"sourcesContent":["<template>\n    <layout-div>\n          <div class=\"container\">\n              <h2 class=\"text-center mt-5 mb-3\">Project Manager</h2>\n              <div class=\"card\">\n                  <div class=\"card-header\">\n                      <router-link to=\"/create\"\n                          class=\"btn btn-outline-primary\"\n                          >Create New Project\n                      </router-link>\n                  </div>\n                  <div class=\"card-body\">\n               \n                      <table class=\"table table-bordered\">\n                          <thead>\n                              <tr>\n                                  <th>Name</th>\n                                  <th>Description</th>\n                                  <th width=\"240px\">Action</th>\n                              </tr>\n                          </thead>\n                          <tbody>\n                               \n                              <tr v-for=\"project in projects\" :key=\"project.id\">\n                                  <td>{{project.name}}</td>\n                                  <td>{{project.description}}</td>\n                                  <td>\n                                      <router-link :to=\"`/show/${project.id}`\" class=\"btn btn-outline-info mx-1\">Show</router-link>\n                                      <router-link :to=\"`/edit/${project.id}`\" class=\"btn btn-outline-success mx-1\">Edit</router-link>\n                                      <button \n                                          @click=\"handleDelete(project.id)\"\n                                          className=\"btn btn-outline-danger mx-1\">\n                                          Delete\n                                      </button>\n                                  </td>\n                              </tr>\n                                   \n                          </tbody>\n                      </table>\n                  </div>\n              </div>\n          </div>\n      </layout-div>\n  </template>\n   \n  <script>\n  import axios from 'axios';\n  import LayoutDiv from '../LayoutDiv.vue';\n  import Swal from 'sweetalert2'\n   \n  export default {\n    name: 'ProjectList',\n    components: {\n      LayoutDiv,\n    },\n    data() {\n      return {\n        posts: [],\n        projects:[]\n      };\n    },\n    created() {\n      this.fetchProjectList();\n    },\n    methods: {\n      fetchProjectList() {\n        axios.get('/api/projects')\n          .then(response => {\n              this.projects = response.data;\n              return response\n          })\n          .catch(error => {\n            return error\n          });\n      },\n      handleDelete(id){\n          Swal.fire({\n              title: 'Are you sure?',\n              text: \"You won't be able to revert this!\",\n              icon: 'warning',\n              showCancelButton: true,\n              confirmButtonColor: '#3085d6',\n              cancelButtonColor: '#d33',\n              confirmButtonText: 'Yes, delete it!'\n            }).then((result) => {\n              if (result.isConfirmed) {\n                  axios.delete(`/api/projects/${id}`)\n                  .then( response => {\n                      Swal.fire({\n                          icon: 'success',\n                          title: 'Project deleted successfully!',\n                          showConfirmButton: false,\n                          timer: 1500\n                      })\n                      this.fetchProjectList();\n                      return response\n                  })\n                  .catch(error => {\n                      Swal.fire({\n                           icon: 'error',\n                          title: 'An Error Occured!',\n                          showConfirmButton: false,\n                          timer: 1500\n                      })\n                      return error\n                  });\n              }\n            })\n      }\n    },\n  };\n  </script>"],"mappings":"AA8CE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,kBAAkB;AACxC,OAAOC,IAAG,MAAO,aAAY;AAE7B,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACPD,gBAAgBA,CAAA,EAAG;MACjBT,KAAK,CAACW,GAAG,CAAC,eAAe,EACtBC,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACN,QAAO,GAAIM,QAAQ,CAACR,IAAI;QAC7B,OAAOQ,QAAO;MAClB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACd,OAAOA,KAAI;MACb,CAAC,CAAC;IACN,CAAC;IACDC,YAAYA,CAACC,EAAE,EAAC;MACZf,IAAI,CAACgB,IAAI,CAAC;QACNC,KAAK,EAAE,eAAe;QACtBC,IAAI,EAAE,mCAAmC;QACzCC,IAAI,EAAE,SAAS;QACfC,gBAAgB,EAAE,IAAI;QACtBC,kBAAkB,EAAE,SAAS;QAC7BC,iBAAiB,EAAE,MAAM;QACzBC,iBAAiB,EAAE;MACrB,CAAC,CAAC,CAACb,IAAI,CAAEc,MAAM,IAAK;QAClB,IAAIA,MAAM,CAACC,WAAW,EAAE;UACpB3B,KAAK,CAAC4B,MAAM,CAAE,iBAAgBX,EAAG,EAAC,EACjCL,IAAI,CAAEC,QAAO,IAAK;YACfX,IAAI,CAACgB,IAAI,CAAC;cACNG,IAAI,EAAE,SAAS;cACfF,KAAK,EAAE,+BAA+B;cACtCU,iBAAiB,EAAE,KAAK;cACxBC,KAAK,EAAE;YACX,CAAC;YACD,IAAI,CAACrB,gBAAgB,CAAC,CAAC;YACvB,OAAOI,QAAO;UAClB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;YACZb,IAAI,CAACgB,IAAI,CAAC;cACLG,IAAI,EAAE,OAAO;cACdF,KAAK,EAAE,mBAAmB;cAC1BU,iBAAiB,EAAE,KAAK;cACxBC,KAAK,EAAE;YACX,CAAC;YACD,OAAOf,KAAI;UACf,CAAC,CAAC;QACN;MACF,CAAC;IACP;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}