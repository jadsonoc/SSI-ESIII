{"ast":null,"code":"import axios from 'axios';\nimport LayoutDiv from './LayoutDiv.vue';\nimport Swal from 'sweetalert2';\nexport default {\n  name: 'RecipeList',\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      posts: [],\n      recipes: []\n    };\n  },\n  created() {\n    axios.get('/recipes/mainSearchByFoods', this.$route.params.query).then(response => {\n      this.recipes = response.data;\n      return response;\n    }).catch(error => {\n      Swal.fire({\n        icon: 'error',\n        //title: 'An Error Occured!',\n        title: error,\n        showConfirmButton: false,\n        timer: 1500\n      });\n      return error;\n    });\n  },\n  methods: {\n    fetchRecipesList() {\n      axios.get('/recipes').then(response => {\n        this.recipes = response.data;\n        return response;\n      }).catch(error => {\n        return error;\n      });\n    },\n    handleDelete(id) {\n      Swal.fire({\n        title: 'Are you sure?',\n        text: \"You won't be able to revert this!\",\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#3085d6',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Yes, delete it!'\n      }).then(result => {\n        if (result.isConfirmed) {\n          axios.delete(`/recipes/${id}`).then(response => {\n            Swal.fire({\n              icon: 'success',\n              title: 'Recipe deleted successfully!',\n              showConfirmButton: false,\n              timer: 1500\n            });\n            this.fetchRecipesList();\n            return response;\n          }).catch(error => {\n            Swal.fire({\n              icon: 'error',\n              title: 'An Error Occured!',\n              showConfirmButton: false,\n              timer: 1500\n            });\n            return error;\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","LayoutDiv","Swal","name","components","data","posts","recipes","created","get","$route","params","query","then","response","catch","error","fire","icon","title","showConfirmButton","timer","methods","fetchRecipesList","handleDelete","id","text","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","isConfirmed","delete"],"sources":["/Volumes/SDDisk2/workspace/IFRS/ES3/SSI-ESIII/cookit/src/main/webapp/src/components/RecipeSearchList.vue"],"sourcesContent":["<template>\n    <layout-div>\n          <div class=\"container\">\n              <h2 class=\"text-center mt-5 mb-3\">Recipe Manager</h2>\n              <div class=\"card\">\n                  <div class=\"card-header\">\n                      <router-link to=\"/recipe/create\"\n                          class=\"btn btn-outline-primary\"\n                          >Create New Recipe\n                      </router-link>\n                  </div>\n                  <div class=\"card-body\">\n               {{ this.$router.query.item  }}\n\n               {{ response  }}\n                      <table class=\"table table-bordered\">\n                          <thead>\n                              <tr>\n                                  <th>Name</th>\n                                  <th>Preparation</th>\n                                  <th>Time</th>\n                                  <th>Serve</th>\n                                  <th>Difficulty</th>\n                                  <th width=\"240px\">Action</th>\n                              </tr>\n                          </thead>\n                          <tbody>\n                               \n                              <tr v-for=\"recipe in recipes\" :key=\"recipe.id\">\n                                  <td>{{recipe.name}}</td>\n                                  <td>{{recipe.preparation}}</td>\n                                  <td>{{recipe.time}}</td>\n                                  <td>{{recipe.serve}}</td>\n                                  <td>{{recipe.difficulty}}</td>\n                                  <td>\n                                      <router-link :to=\"`/show/${recipe.id}`\" class=\"btn btn-outline-info mx-1\">Show</router-link>\n                                      <router-link :to=\"`/edit/${recipe.id}`\" class=\"btn btn-outline-success mx-1\">Edit</router-link>\n                                      <button \n                                          @click=\"handleDelete(recipe.id)\"\n                                          className=\"btn btn-outline-danger mx-1\">\n                                          Delete\n                                      </button>\n                                  </td>\n                              </tr>\n                                   \n                          </tbody>\n                      </table>\n                  </div>\n              </div>\n          </div>\n      </layout-div>\n  </template>\n   \n  <script>\n  import axios from 'axios';\n  import LayoutDiv from './LayoutDiv.vue';\n  import Swal from 'sweetalert2'\n   \n  export default {\n    name: 'RecipeList',\n    components: {\n      LayoutDiv,\n    },\n    data() {\n      return {\n        posts: [],\n        recipes:[]\n      };\n    },\n    created() {\n        axios.get('/recipes/mainSearchByFoods',this.$route.params.query)\n          .then(response => {\n            this.recipes = response.data;\n            return response\n          })\n          .catch(error => {\n            Swal.fire({\n              icon: 'error',\n              //title: 'An Error Occured!',\n              title: error,\n              showConfirmButton: false,\n              timer: 1500\n            })\n            return error\n          });\n    },\n    methods: {\n        fetchRecipesList() {\n        axios.get('/recipes')\n          .then(response => {\n              this.recipes = response.data;\n              return response\n          })\n          .catch(error => {\n            return error\n          });\n      },\n      handleDelete(id){\n          Swal.fire({\n              title: 'Are you sure?',\n              text: \"You won't be able to revert this!\",\n              icon: 'warning',\n              showCancelButton: true,\n              confirmButtonColor: '#3085d6',\n              cancelButtonColor: '#d33',\n              confirmButtonText: 'Yes, delete it!'\n            }).then((result) => {\n              if (result.isConfirmed) {\n                  axios.delete(`/recipes/${id}`)\n                  .then( response => {\n                      Swal.fire({\n                          icon: 'success',\n                          title: 'Recipe deleted successfully!',\n                          showConfirmButton: false,\n                          timer: 1500\n                      })\n                      this.fetchRecipesList();\n                      return response\n                  })\n                  .catch(error => {\n                      Swal.fire({\n                           icon: 'error',\n                          title: 'An Error Occured!',\n                          showConfirmButton: false,\n                          timer: 1500\n                      })\n                      return error\n                  });\n              }\n            })\n      }\n    },\n  };\n  </script>"],"mappings":"AAsDE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,IAAG,MAAO,aAAY;AAE7B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAC;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNR,KAAK,CAACS,GAAG,CAAC,4BAA4B,EAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,KAAK,EAC5DC,IAAI,CAACC,QAAO,IAAK;MAChB,IAAI,CAACP,OAAM,GAAIO,QAAQ,CAACT,IAAI;MAC5B,OAAOS,QAAO;IAChB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;MACdd,IAAI,CAACe,IAAI,CAAC;QACRC,IAAI,EAAE,OAAO;QACb;QACAC,KAAK,EAAEH,KAAK;QACZI,iBAAiB,EAAE,KAAK;QACxBC,KAAK,EAAE;MACT,CAAC;MACD,OAAOL,KAAI;IACb,CAAC,CAAC;EACR,CAAC;EACDM,OAAO,EAAE;IACLC,gBAAgBA,CAAA,EAAG;MACnBvB,KAAK