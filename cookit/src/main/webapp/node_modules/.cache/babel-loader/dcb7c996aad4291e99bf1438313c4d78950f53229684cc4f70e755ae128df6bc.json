            <th>Ingredient</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"ingredient in recipe.ingredients\" :key=\"ingredient.id\">\n              <td>{{ ingredient.quantity }}</td>\n              <td>{{ ingredient.food.foodUnit }}</td>\n              <td>{{ ingredient.food.name }}</td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </layout-div>\n</template>\n\n<script>\nimport VueCookies from 'vue-cookies';\nimport axios from \"axios\";\nimport NavMenu from \"../components/NavMenu.vue\";\nimport LayoutDiv from \"../components/LayoutDiv.vue\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  name: \"RecipeShow\",\n  components: {\n    NavMenu,\n    LayoutDiv,\n  },\n  data() {\n    return {\n      recipe: {\n        name: \"\",\n        preparation: \"\",\n        time: \"\",\n        serve: \"\",\n        difficulty: \"\",\n        ingredients: [],\n      },\n      isLogged: false,\n    };\n  },\n  created() {\n    if (VueCookies.isKey('userId')) {\n      this.isLogged = true;\n    }\n    const id = this.$route.params.id;\n    axios\n      .get(`/recipes/${id}`)\n      .then((response) => {\n        let recipeInfo = response.data;\n        this.recipe.name = recipeInfo.name;\n        this.recipe.preparation = recipeInfo.preparation;\n        this.recipe.time = recipeInfo.time;\n        this.recipe.serve = recipeInfo.serve;\n        this.recipe.difficulty = recipeInfo.difficulty;\n        this.recipe.ingredients = recipeInfo.ingredients;\n        return response;\n      })\n      .catch((error) => {\n        Swal.fire({\n          icon: \"error\",\n          title: \"An Error Occured Getting the Recipe!\",\n          showConfirmButton: false,\n          timer: 1500,\n        });\n        return error;\n      });\n  },\n  methods: {\n    handleFavourite() {\n      const userId = VueCookies.get('userId');\n      console.log(userId);\n      console.log(this.recipe);\n          axios\n            .post(`/userManager/favourite/${userId}`, this.recipe)\n            .then((response) => {\n              Swal.fire({\n                icon: \"success\",\n                title: \"Recipe favourited successfully!\",\n                showConfirmButton: false,\n                timer: 1500,\n              });\n              return response;\n            })\n            .catch((error) => {\n              Swal.fire({\n                icon: \"error\",\n                title: \"An Error Occured!\",\n                showConfirmButton: false,\n                timer: 1500,\n              });\n              return error;\n            });\n        },\n  },\n};\n</script>\n"],"mappings":"AA8DA,OAAOA,UAAS,MAAO,aAAa;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,2BAA2B;AAC/C,OAAOC,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVJ,OAAO;IACPC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNH,IAAI,EAAE,EAAE;QACRI,WAAW,EAAE,EAAE;QACfC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,UAAU,EAAE,EAAE;QACdC,WAAW,EAAE;MACf,CAAC;MACDC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAIf,UAAU,CAACgB,KAAK,CAAC,QAAQ,CAAC,EAAE;MAC9B,IAAI,CAACF,QAAO,GAAI,IAAI;IACtB;IACA,MAAMG,EAAC,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,EAAE;IAChChB,KAAI,CACDmB,GAAG,CAAE,YAAWH,EAAG,EAAC,EACpBI,IAAI,CAAEC,QAAQ,IAAK;MAClB,IAAIC,UAAS,GAAID,QAAQ,CAACf,IAAI;MAC9B,IAAI,CAACC,MAAM,CAACH,IAAG,GAAIkB,UAAU,CAAClB,IAAI;MAClC,IAAI,CAACG,MAAM,CAACC,WAAU,GAAIc,UAAU,CAACd,WAAW;MAChD,IAAI,CAACD,MAAM,CAACE,IAAG,GAAIa,UAAU,CAACb,IAAI;MAClC,IAAI,CAACF,MAAM,CAACG,KAAI,GAAIY,UAAU,CAACZ,KAAK;MACpC,IAAI,CAACH,MAAM,CAACI,UAAS,GAAIW,UAAU,CAACX,UAAU;MAC9C,IAAI,CAACJ,MAAM,CAACK,WAAU,GAAIU,UAAU,CAACV,WAAW;MAChD,OAAOS,QAAQ;IACjB,CAAC,EACAE,KAAK,CAAEC,KAAK,IAAK;MAChBrB,IAAI,CAACsB,IAAI,CAAC;QACRC,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE,sCAAsC;QAC7CC,iBAAiB,EAAE,KAAK;QACxBC,KAAK,EAAE;MACT,CAAC,CAAC;MACF,OAAOL,KAAK;IACd,CAAC,CAAC;EACN,CAAC;EACDM,OAAO,EAAE;IACPC,eAAeA,CAAA,