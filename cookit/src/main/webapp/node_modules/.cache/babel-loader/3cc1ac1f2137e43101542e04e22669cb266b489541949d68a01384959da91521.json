{"ast":null,"code":"import axios from \"axios\";\nimport NavMenu from './NavMenu.vue';\nimport LayoutDiv from \"../components/LayoutDiv.vue\";\nimport Swal from \"sweetalert2\";\nexport default {\n  name: \"RecipeList\",\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      posts: [],\n      recipes: []\n    };\n  },\n  created() {\n    this.fetchRecipesList();\n  },\n  methods: {\n    fetchRecipesList() {\n      axios.get(\"/recipes\").then(response => {\n        this.recipes = response.data;\n        return response;\n      }).catch(error => {\n        return error;\n      });\n    },\n    handleDelete(id) {\n      Swal.fire({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\"\n      }).then(result => {\n        if (result.isConfirmed) {\n          axios.delete(`/recipes/${id}`).then(response => {\n            Swal.fire({\n              icon: \"success\",\n              title: \"Recipe deleted successfully!\",\n              showConfirmButton: false,\n              timer: 1500\n            });\n            this.fetchRecipesList();\n            return response;\n          }).catch(error => {\n            Swal.fire({\n              icon: \"error\",\n              title: \"An Error Occured!\",\n              showConfirmButton: false,\n              timer: 1500\n            });\n            return error;\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","NavMenu","LayoutDiv","Swal","name","components","data","posts","recipes","created","fetchRecipesList","methods","get","then","response","catch","error","handleDelete","id","fire","title","text","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","isConfirmed","delete","showConfirmButton","timer"],"sources":["/Volumes/SDDisk2/workspace/IFRS/ES3/SSI-ESIII/cookit/src/main/webapp/src/views/RecipeList.vue"],"sourcesContent":["<template>\n  <NavMenu />\n  <layout-div>\n    <div class=\"container\">\n      <h2 class=\"text-center mt-5 mb-3\">Recipe Manager</h2>\n      <div class=\"card\">\n        <div class=\"card-header\">\n          <router-link to=\"/recipe/create\" class=\"btn btn-outline-primary\"\n            >Create New Recipe\n          </router-link>\n        </div>\n        <div class=\"card-body\">\n          <table class=\"table table-bordered\">\n            <thead>\n              <tr>\n                <th>Name</th>\n                <th>Preparation</th>\n                <th>Time</th>\n                <th>Serve</th>\n                <th>Difficulty</th>\n                <th width=\"240px\">Action</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"recipe in recipes\" :key=\"recipe.id\">\n                <td>{{ recipe.name }}</td>\n                <td>{{ recipe.preparation }}</td>\n                <td>{{ recipe.time }}</td>\n                <td>{{ recipe.serve }}</td>\n                <td>{{ recipe.difficulty }}</td>\n                <td>\n                  <router-link\n                    :to=\"`/recipe/show/${recipe.id}`\"\n                    class=\"btn btn-outline-info mx-1\"\n                    >Show</router-link\n                  >\n                  <router-link\n                    :to=\"`/recipe/edit/${recipe.id}`\"\n                    class=\"btn btn-outline-success mx-1\"\n                    >Edit</router-link\n                  >\n                  <button\n                    @click=\"handleDelete(recipe.id)\"\n                    className=\"btn btn-outline-danger mx-1\"\n                  >\n                    Delete\n                  </button>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </layout-div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport NavMenu from './NavMenu.vue' \nimport LayoutDiv from \"../components/LayoutDiv.vue\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  name: \"RecipeList\",\n  components: {\n    LayoutDiv,\n  },\n  data() {\n    return {\n      posts: [],\n      recipes: [],\n    };\n  },\n  created() {\n    this.fetchRecipesList();\n  },\n  methods: {\n    fetchRecipesList() {\n      axios\n        .get(\"/recipes\")\n        .then((response) => {\n          this.recipes = response.data;\n          return response;\n        })\n        .catch((error) => {\n          return error;\n        });\n    },\n    handleDelete(id) {\n      Swal.fire({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\",\n      }).then((result) => {\n        if (result.isConfirmed) {\n          axios\n            .delete(`/recipes/${id}`)\n            .then((response) => {\n              Swal.fire({\n                icon: \"success\",\n                title: \"Recipe deleted successfully!\",\n                showConfirmButton: false,\n                timer: 1500,\n              });\n              this.fetchRecipesList();\n              return response;\n            })\n            .catch((error) => {\n              Swal.fire({\n                icon: \"error\",\n                title: \"An Error Occured!\",\n                showConfirmButton: false,\n                timer: 1500,\n              });\n              return error;\n            });\n        }\n      });\n    },\n  },\n};\n</script>\n"],"mappings":"AA0DA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,eAAc;AAClC,OAAOC,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACL