{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport LayoutDiv from \"./LayoutDiv.vue\";\nimport Swal from \"sweetalert2\";\nexport default {\n  name: \"RecipeList\",\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      ingredients: [],\n      recipes: []\n    };\n  },\n  created() {\n    const param = JSON.parse(this.$route.params.query);\n    param.forEach(ingredient => {\n      console.log(ingredient);\n      this.ingredients.push(ingredient);\n    });\n    axios.post(\"/recipes/mainSearchByFoods\", this.ingredients).then(response => {\n      console.log(response);\n      if (response.data.length === 0) {\n        Swal.fire({\n          icon: \"warning\",\n          title: \"Ops! Unfortunatelly, we don't have any recipe with those ingredients in our database\",\n          showConfirmButton: false,\n          timer: 5500\n        });\n      } else {\n        let recipesInfo = response.data;\n        recipesInfo.forEach(element => {\n          this.recipes.push(element);\n        });\n      }\n      return response;\n    }).catch(error => {\n      Swal.fire({\n        icon: \"error\",\n        //title: 'An Error Occured!',\n        title: error,\n        showConfirmButton: false,\n        timer: 1500\n      });\n      return error;\n    });\n  }\n};","map":{"version":3,"names":["axios","LayoutDiv","Swal","name","components","data","ingredients","recipes","created","param","JSON","parse","$route","params","query","forEach","ingredient","console","log","push","post","then","response","length","fire","icon","title","showConfirmButton","timer","recipesInfo","element","catch","error"],"sources":["/Volumes/SDDisk2/workspace/IFRS/ES3/SSI-ESIII/cookit/src/main/webapp/src/components/RecipeSearchList.vue"],"sourcesContent":["<template>\n  <layout-div>\n    <div class=\"container\">\n      <h2 class=\"text-center mt-5 mb-3\">Recipe Suggestions</h2>\n      <div class=\"card\">\n        <div class=\"card-header\">\n          <router-link to=\"/\" class=\"btn btn-outline-primary\"\n            >Go back to Home\n          </router-link>\n        </div>\n        <div class=\"card-body\">\n          <table class=\"table table-bordered\">\n            <thead>\n              <tr>\n                <th>Name</th>\n                <th>Preparation</th>\n                <th>Time</th>\n                <th>Serve</th>\n                <th>Difficulty</th>\n                <th width=\"240px\">Action</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"recipe in recipes\" :key=\"recipe.id\">\n                <td>{{ recipe.name }}</td>\n                <td>{{ recipe.preparation }}</td>\n                <td>{{ recipe.time }}</td>\n                <td>{{ recipe.serve }}</td>\n                <td>{{ recipe.difficulty }}</td>\n                <td>\n                  <router-link\n                    :to=\"`/show/${recipe.id}`\"\n                    class=\"btn btn-outline-info mx-1\"\n                    >Show</router-link\n                  >\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </layout-div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport LayoutDiv from \"./LayoutDiv.vue\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  name: \"RecipeList\",\n  components: {\n    LayoutDiv,\n  },\n  data() {\n    return {\n      ingredients: [],\n      recipes: [],\n    };\n  },\n  created() {\n    const param = JSON.parse(this.$route.params.query);\n    param.forEach((ingredient) => {\n      console.log(ingredient);\n      this.ingredients.push(ingredient);\n    });\n    axios\n      .post(\"/recipes/mainSearchByFoods\", this.ingredients)\n      .then((response) => {\n        console.log(response)\n        if (response.data.length === 0) {\n          Swal.fire({\n            icon: \"warning\",\n            title:\n              \"Ops! Unfortunatelly, we don't have any recipe with those ingredients in our database\",\n            showConfirmButton: false,\n            timer: 5500,\n          });\n        } else {\n          let recipesInfo = response.data;\n          recipesInfo.forEach((element) => {\n            this.recipes.push(element);\n          });\n        }\n        return response;\n      })\n      .catch((error) => {\n        Swal.fire({\n          icon: \"error\",\n          //title: 'An Error Occured!',\n          title: error,\n          showConfirmButton: false,\n          timer: 1500,\n        });\n        return error;\n      });\n  },\n};\n</script>\n"],"mappings":";AA8CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,KAAI,GAAIC,IAAI,CAACC,KAAK,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,KAAK,CAAC;IAClDL,KAAK,CAACM,OAAO,CAAEC,UAAU,IAAK;MAC5BC,OAAO,CAACC,GAAG,CAACF,UAAU,CAAC;MACvB,IAAI,CAACV,WAAW,CAACa,IAAI,CAACH,UAAU,CAAC;IACnC,CAAC,CAAC;IACFhB,KAAI,CACDoB,IAAI,CAAC,4BAA4B,EAAE,IAAI,CAACd,WAAW,EACnDe,IAAI,CAAEC,QAAQ,IAAK;MAClBL,OAAO,CAACC,GAAG,CAACI,QAAQ;MACpB,IAAIA,QAAQ,CAACjB,IAAI,CAACkB,MAAK,KAAM,CAAC,EAAE;QAC9BrB,IAAI,CAACsB,IAAI,CAAC;UACRC,IAAI,EAAE,SAAS;UACfC,KAAK,EACH,sFAAsF;UACxFC,iBAAiB,EAAE,KAAK;UACxBC,KAAK,EAAE;QACT,CAAC,CAAC;MACJ,OAAO;QACL,IAAIC,WAAU,GAAIP,QAAQ,CAACjB,IAAI;QAC/BwB,WAAW,CAACd,OAAO,CAAEe,OAAO,IAAK;UAC/B,IAAI,CAACvB,OAAO,CAACY,IAAI,CAACW,OAAO,CAAC;QAC5B,CAAC,CAAC;MACJ;MACA,OAAOR,QAAQ;IACjB,CAAC,EACAS,KAAK,CAAEC,KAAK,IAAK;MAChB9B,IAAI,CAACsB,IAAI,CAAC;QACRC,IAAI,EAAE,OAAO;QACb;QACAC,KAAK,EAAEM,KAAK;QACZL,iBAAiB,EAAE,KAAK;QACxBC,KAAK,EAAE;MACT,CAAC,CAAC;MACF,OAAOI,KAAK;IACd,CAAC,CAAC;EACN;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}